@inherits BaseLayoutPage
<AuthorizeView>
	<NotAuthorized>
		<LoginRedirect />
	</NotAuthorized>
	<Authorizing>
		<Toast Message="@LG("Auth.CredentialsCheck")" />
	</Authorizing>
	<Authorized>
		<CascadingValue Value="@DaxState" Name="DaxStateValue">
			@Body
		</CascadingValue>
	</Authorized>
</AuthorizeView>
